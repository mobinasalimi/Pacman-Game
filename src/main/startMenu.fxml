<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StartMenu">
   <children>
      <ImageView fitHeight="506.0" fitWidth="504.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../manabe/Pacman2x2-HomeOffice-blue_800x.jpg" />
         </image>
      </ImageView>
      <Label layoutX="92.0" layoutY="14.0" prefHeight="34.0" prefWidth="338.0" text="Welcome To Packman" textFill="#a3dcff">
         <font>
            <Font name="Berlin Sans FB" size="36.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="19.0" layoutY="214.0" mnemonicParsing="false" onAction="#loginOnAction" prefHeight="46.0" prefWidth="134.0" text="Login" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="playAsGuestButton" layoutX="19.0" layoutY="284.0" mnemonicParsing="false" onAction="#playAsGuestAction" prefHeight="46.0" prefWidth="134.0" text="Play As Guest" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="registerButton" layoutX="19.0" layoutY="355.0" mnemonicParsing="false" onAction="#registerOnAction" prefHeight="46.0" prefWidth="134.0" text="Register" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="settingButton" layoutX="355.0" layoutY="214.0" mnemonicParsing="false" onAction="#settingOnAction" prefHeight="46.0" prefWidth="134.0" text="Setting" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="355.0" layoutY="355.0" mnemonicParsing="false" onAction="#exitOnAction" prefHeight="46.0" prefWidth="134.0" text="Exit" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Button>
      <Button fx:id="scoreboardButton" layoutX="355.0" layoutY="284.0" mnemonicParsing="false" onAction="#scoreboardOnAction" prefHeight="46.0" prefWidth="134.0" text="Scoreboard" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
