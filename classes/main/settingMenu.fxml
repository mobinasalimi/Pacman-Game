<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SettingMenu">
   <children>
      <ImageView fitHeight="512.0" fitWidth="518.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../manabe/Pacman2x2-Brick-lightblue_1024x.jpg" />
         </image>
      </ImageView>
      <Label layoutX="174.0" layoutY="25.0" text="Setting" textFill="#ff52aa">
         <font>
            <Font name="Showcard Gothic" size="38.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="19.0" layoutY="332.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="63.0" prefWidth="154.0" text="Back" textFill="#43768e">
         <font>
            <Font name="Comic Sans MS" size="26.0" />
         </font>
      </Button>
      <Button layoutX="19.0" layoutY="236.0" mnemonicParsing="false" onAction="#accountOnAction" prefHeight="58.0" prefWidth="154.0" text="Account" textFill="#43768e">
         <font>
            <Font name="Comic Sans MS" size="30.0" />
         </font>
      </Button>
      <Button layoutX="20.0" layoutY="141.0" mnemonicParsing="false" onAction="#lpAndManpOnAction" prefHeight="58.0" prefWidth="153.0" text="LP &amp; MAPS" textFill="#43768e">
         <font>
            <Font name="Comic Sans MS" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
